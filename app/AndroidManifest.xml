<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yoan.game.tictactoe"
    android:versionCode="0"
    android:versionName="0.0.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="16" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="yoan.game.tictactoe.game.context.GlobalContext"
        android:allowBackup="true" >
        <activity
            android:name="yoan.game.tictactoe.activities.MainActivity"
            android:label="@string/title_activity_accueil" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="yoan.game.tictactoe.activities.GameActivity" />
    </application>

</manifest>